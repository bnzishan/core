PREFIX hobbit: <http://w3id.org/hobbit/vocab#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

DELETE { GRAPH %GRAPH_URI%     { ?task hobbit:involvesSystemInstance ?system . }}
INSERT { GRAPH %NEW_GRAPH_URI% { ?task hobbit:involvesSystemInstance ?system . }}
USING %GRAPH_URI%
WHERE {
    ?task a hobbit:ChallengeTask ;
            hobbit:isTaskOf %CHALLENGE_URI% ;
            hobbit:involvesSystemInstance ?system .
}
